---
import BaseHead from "../components/BaseHead.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";

const page_title = "Astro勉強中";
const description = "AstroというJSフレームワークの勉強しています。";
const type = "website";
---

<html lang='ja'>
  <head>
    <BaseHead title={page_title} description={description} type={type} />
  </head>
  <body>
    <Header />
    <main>
      <section class='mv'>
        <h1>{page_title}</h1>
        <p></p>
      </section>
      <section class='form_area'></section>
    </main>
    <Footer />
    <style>
      main {
        padding-top: 60px;
      }
    </style>
  </body>
</html>

